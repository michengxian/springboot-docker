<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootdocker.mapper.EmpMapper">

    <select id="getEmpList" resultType="com.example.springbootdocker.bean.EmpBean">
        select
            empno as empNo,
            ename as eName,
            job as job,
            deptno as deptNo
         from emp
    </select>

</mapper>